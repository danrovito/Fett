{
    "name": "danrovito/fett",
    "description": "An API framework based on Slim PHP",
    "keywords": ["framework", "slim", "fett"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Dan Rovito",
            "email": "dan@danrovito.com"
        }
    ],
    "require": {
        "illuminate/database": "^5.8",
        "slim/slim": "^3.12",
        "symfony/var-dumper": "^4.3",
        "vlucas/phpdotenv": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
